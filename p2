#include <iostream>
using namespace std;

int main() {
    int matrix[3][3] = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };

    cout << "The 3x3 Matrix is:\n";

    for (int i = 0; i < 3; i++) {        // Row loop
        for (int j = 0; j < 3; j++) {    // Column loop
            cout << matrix[i][j] << " ";
        }
        cout << endl;  // Move to next line after each row
    }

    return 0;
}
